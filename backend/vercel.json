{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "^/api/v1/users/(.*)", "dest": "/api/v1/users/$1", "methods": ["GET", "POST","PUT","DELETE"],"headers":{"Access-Control-Allow-Origin": "*"}},
    { "src": "^/api/v1/events/(.*)", "dest": "/api/v1/events/$1", "methods": ["GET", "POST","PUT","DELETE"],"headers":{"Access-Control-Allow-Origin": "*"} },
    { "src": "^/api/v1/attendees/(.*)", "dest": "/api/v1/attendees/$1", "methods": ["GET", "POST","PUT","DELETE"],"headers":{"Access-Control-Allow-Origin": "*"} },
    { "src": "/(.*)", "dest": "/", "methods": ["GET", "POST","PUT","DELETE"],"headers":{"Access-Control-Allow-Origin": "*"}}
  ]
}
