{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "^/api/v1/users/(.*)", "dest": "/api/v1/users/$1" },
    { "src": "^/api/v1/events/(.*)", "dest": "/api/v1/events/$1" },
    { "src": "^/api/v1/attendees/(.*)", "dest": "/api/v1/attendees/$1" },
    { "src": "/(.*)", "dest": "/" }
  ]
}
